#!/Users/ant/code/dj/.dj/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'dj','console_scripts','dj'
__requires__ = 'dj'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('dj', 'console_scripts', 'dj')()
    )
